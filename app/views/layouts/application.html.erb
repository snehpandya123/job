<!DOCTYPE html>
<html>
<head>
  <title>Job</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <script src="//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"></script>
  <%= csrf_meta_tags %>
</head>
<body>



<div class="navbar-wrapper">
  

        <div class="navbar navbar-inverse navbar-static-top" role="navigation">
          <div class="container">
            <div class="navbar-header">
              <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
              </button>
              <%= link_to "ChallengeMe" , users_dashboard_path , :class => "navbar-brand" %>
            
            </div>
            <div class="navbar-collapse collapse">
              <ul class="nav navbar-nav">
                <li><%= link_to "Home" , users_dashboard_path %></li>
                <% if signed_in? %>
                <li><a href="#about">About</a></li>
                <li><%= link_to "Profile" , "#" %></li>
                <% end %>
                
                

              </ul>
               <ul class="nav navbar-nav navbar-right">
                <% if signed_in? %>

                <li><%= link_to "signout" ,destroy_user_session_path , :method=>:delete , :class => "btn btn-default"%></li>
                <% else %>
                <li><%= link_to "Sign Up" , new_user_registration_path , :class => "btn btn-default" %></li>
                <li><%= link_to "Sign In" , new_user_session_path , :class => "btn btn-default"%></li>
                <% end %>
                  
                </ul>
            </div>
          </div>
      

      </div>



<%= yield %>
<div class = "container">
    <footer>
        <p class="pull-right"><a href="#">Back to top</a></p>
        <p>&copy; 2014 Pandya Software, Inc. &middot; </p>
      </footer>
      
</div>

</body>
</html>
